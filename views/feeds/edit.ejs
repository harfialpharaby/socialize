<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        <link rel="stylesheet" href="/stylesheets/bootstrap-tagsinput.css">
        <link rel="stylesheet" href="/stylesheets/bootstrap-tagsinput-typeahead.css">

        <title>Hello, world!</title>
    </head>
    <body>
        <%-include('../partials/navbar')%>

        <%=JSON.stringify(feed)%>
        <form method="post">
            TITLE: <input type="text" name="title" id="title" value="<%=feed.title%>"><br>
            CONTENT: <input type="textara" name="content" id="content" value="<%=feed.content%>"><br>
            TAG: <input type="tag" name="tagsName" id="tagsName"><br>
            <button type="submit">Save</button>
        </form>

        <%-include('../partials/footer')%>
    </body>
</html>